{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": [
    "react",
    "typescript",
    "unicorn",
    "promise",
    "oxc",
    "jsdoc",
    "jsx-a11y"
  ],
  "ignore_patterns": [
    "**/node_modules",
    "**/build",
    "**/dist",
    "**/.react-router",
    "*.config.js",
    "*.config.ts"
  ],
  "rules": {}
}
